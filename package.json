{
  "name": "grc-ui-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf output && mkdir output",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext ,.js .",
    "lint:fix": "eslint --ext .jsx,.js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/grc-ui-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^1.3.5",
    "body-parser": "^1.19.0",
    "braces": "^3.0.2",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^5.2.1",
    "express": "^4.17.1",
    "flat": "^4.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "graphql-type-json": "^0.2.4",
    "helmet": "^3.22.0",
    "js-yaml": "^3.13.1",
    "jws": "^3.2.2",
    "lodash": "^4.17.15",
    "log4js": "^4.5.1",
    "lru-cache": "^4.1.3",
    "moment": "^2.25.3",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "requestretry": "^1.13.0",
    "security-middleware": "github:open-cluster-management/security-middleware"
  },
  "devDependencies": {
    "ajv": "^6.12.2",
    "ajv-keywords": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "lint-staged": "^7.3.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.4",
    "supertest": "^3.1.0"
  }
}
